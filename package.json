{
  "name": "fitness-rpg",
  "version": "1.0.0",
  "description": "A fitness RPG powered by Strava activities",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "cf-typegen": "wrangler types",
    "init-db": "wrangler d1 execute fitness-rpg-db --remote --file=./schema.sql",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "webhook:subscribe": "tsx scripts/subscribe-webhook.ts",
    "webhook:list": "tsx scripts/subscribe-webhook.ts list",
    "webhook:unsubscribe": "tsx scripts/subscribe-webhook.ts unsubscribe",
    "sync:all-users": "tsx scripts/force-sync-all-users.ts"
  },
  "dependencies": {
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240925.0",
    "@vitest/ui": "^3.2.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4",
    "wrangler": "^4.54.0"
  }
}
